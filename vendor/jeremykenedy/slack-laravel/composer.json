{
    "name": "jeremykenedy/slack-laravel",
    "description": "Laravel integration for the jeremykenedy/slack package, including facades and service providers.",
    "keywords": ["slack", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "maknz",
            "email": "github@mak.geek.nz"
        },
        {
            "name": "jeremykenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "jeremykenedy/slack": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\Slack\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\Slack\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Slack": "jeremykenedy\\Slack\\Laravel\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
